{
    "name": "aparium-uikit",
    "title": "Aparium UIkit",
    "description": "Our version of the UIkit which is a lightweight and modular front-end framework for developing fast and powerful web interfaces.",
    "version": "3.21.16",
    "main": "dist/js/uikit.js",
    "style": "dist/css/uikit.css",
    "sideEffects": [
        "*.css",
        "./src/js/*.js",
        "./dist/**/*.js"
    ],
    "scripts": {
        "build-scss": "node build/scss",
        "compile": "pnpm compile-less && pnpm compile-js",
        "compile-all": "pnpm icons && pnpm compile-rtl && pnpm compile-less && pnpm compile-js",
        "compile-js": "node build/build",
        "compile-less": "pnpm icons && node build/less",
        "compile-rtl": "pnpm compile-less rtl",
        "format-html": "prettier --write \"apariumUikit/dist/**/*.html\"",
        "generate-favicon": "gulp generate-favicons",
        "icons": "node build/icons",
        "inject-favicon": "gulp inject-favicon-markups",
        "prefix": "node build/prefix",
        "publish-github": "node build/prepare-push-to-github",
        "scope": "node build/scope",
        "release": "node build/release",
        "watch": "chokidar \"**/*.less\" --initial -i \"node_modules\" -c \"node build/less\""
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/aparium/uikit.git",
        "forked-from": "git+https://github.com/uikit/uikit.git"
    },
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/aparium/uikit/issues"
    },
    "homepage": "https://aparium.github.io/aparium-uikit/",
    "credit": {
        "original": "https://getuikit.com",
        "note": "This is a forked version of UIkit, tailored for Aparium Hotel Group. https://getuikit.com"
    },
    "packageManager": "pnpm@9.8.0",
    "devDependencies": {
        "@eslint/js": "^9.15.0",
        "@realfavicongenerator/generate-favicon": "^0.4.12",
        "@realfavicongenerator/gulp-real-favicon": "^0.4.12",
        "@realfavicongenerator/image-adapter-node": "^0.4.12",
        "@rollup/plugin-alias": "^5.1.1",
        "@rollup/plugin-replace": "^6.0.1",
        "archiver": "^7.0.1",
        "camelcase": "^8.0.0",
        "chokidar-cli": "^3.0.0",
        "clean-css": "^5.3.3",
        "dateformat": "^5.0.3",
        "esbuild": "^0.24.0",
        "eslint": "^9.15.0",
        "eslint-config-prettier": "^9.1.0",
        "execa": "^9.5.1",
        "fs-extra": "^11.2.0",
        "glob": "^10.4.5",
        "globals": "^15.12.0",
        "glov": "^0.0.1",
        "gulp": "^5.0.0",
        "inquirer": "^12.1.0",
        "less": "^4.2.0",
        "minimist": "^1.2.8",
        "number-precision": "^1.6.0",
        "p-limit": "^6.1.0",
        "prettier": "^3.3.3",
        "prettier-plugin-organize-imports": "^4.1.0",
        "rollup": "^4.27.4",
        "rollup-plugin-esbuild": "^6.1.1",
        "rollup-plugin-modify": "^3.0.0",
        "rtlcss": "^4.3.0",
        "semver": "^7.6.3",
        "strip-css-comments": "^5.0.0",
        "svgo": "^3.3.2"
    }
}